{
    "manifest_version": 2,
    "name": "CoppShop",
    "version": "1.0",
    "description": "An extension that looks for necessary shopify buttons and clicks them.",
    "permissions": ["storage", "webNavigation", "activeTa", "tabs", "*://*/*"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "coppshop.html"
    }
  }